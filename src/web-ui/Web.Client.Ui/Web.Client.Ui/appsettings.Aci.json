{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  //In this case, we need to specify the port, since the contaiers will be running on the same host and port,
  //For the ACI we need to specify on the docker-compose file the Env variable `ASPNETCORE_URLS` to change it.
  "ProductApiConfig:Host": "product-api:81",
  "UserApiConfig:Host": "user-api:82"
}
